{
  "name": "@trump-fun/bot",
  "version": "1.0.0",
  "module": "bot.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch bot.ts",
    "build": "bun build bot.ts --target=bun",
    "start": "bun bot.ts",
    "deploy": "sh deploy.sh",
    "start:prod": "bun run start:prod --env=production",
    "start:dev": "bun run bot.ts --env=development"
  },
  "devDependencies": {
    "typescript": "^5.8.2",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@trump-fun/common": "workspace:*",
    "telegraf": "^4.16.3",
    "@apollo/client": "^3.13.5",
    "@apollo/server": "^4.11.3",
    "@privy-io/server-auth": "^1.19.1",
    "@supabase/supabase-js": "^2.49.3",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "grammy": "^1.35.1",
    "graphql": "^16.10.0",
    "react": "^19.0.0",
    "supabase": "^2.20.5",
    "viem": "^2.24.1"
  }
}
